# ObfusQ Environment Configuration

# Server port (default: 3000)
PORT=3000

# Node environment
NODE_ENV=production

# Lua executable path
LUA_PATH=/usr/bin/lua5.1

# Max request body size (default: 10mb)
MAX_REQUEST_SIZE=10mb

# Obfuscation timeout in milliseconds (default: 30000)
OBFUSCATION_TIMEOUT=30000

# Enable request logging (default: true)
ENABLE_LOGGING=true
